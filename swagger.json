{
  "swagger": "2.0",
  "info": {
    "title": "Sample Register API",
    "description": "Sample API",
    "version": "1.0.6"
  },
  "securityDefinitions": {
    "JWTAuth": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header",
      "description": "JWT Authorization"
    }
  },
  "paths": {
    "/register": {
      "post": {
        "summary": "Upload Your Details",
        "produces": ["application/json"],
        "parameters": [
          {
            "name": "EmailId",
            "in": "formData",
            "description": "Enter Email Id",
            "type": "string"
          },
          {
            "name": "FirstName",
            "in": "formData",
            "description": "Enter First Name",
            "type": "string"
          },
          {
            "name": "LastName",
            "in": "formData",
            "description": "Enter Last Name",
            "type": "string"
          },
          {
            "name": "Password",
            "in": "formData",
            "description": "Enter Password",
            "type": "string"
          },
          {
            "name": "DoB",
            "in": "formData",
            "description": "Enter Date of Birth",
            "required": false,
            "type": "string",
            "format": "date"
          },
          {
            "name": "PhoneNum",
            "in": "formData",
            "description": "Enter Phone Number",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Registration Successfully"
          }
        }
      }
    },
    "/login": {
      "post": {
        "summary": "Login Your Account",
        "produces": ["application/json"],
        "parameters": [
          {
            "name": "EmailId",
            "in": "formData",
            "description": "Enter Email Id",
            "required": false,
            "type": "string"
          },
          {
            "name": "Password",
            "in": "formData",
            "description": "Enter Password",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Login Successfully"
          }
        }
      }
    },
    "/home": {
      "get": {
        "summary": "Show Your Details",
        "produces": ["application/json"],
        "security": [
          {
            "JWTAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Home Data Retrieval"
          }
        }
      }
    },
    "/edite": {
      "put": {
        "summary": "Update Your Details",
        "produces": ["application/json"],
        "security": [
          {
            "JWTAuth": []
          }
        ],
        "parameters": [
          {
            "name": "EmailId",
            "in": "formData",
            "description": "Enter Email Id",
            "type": "string"
          },
          {
            "name": "FirstName",
            "in": "formData",
            "description": "Enter First Name",
            "type": "string"
          },
          {
            "name": "LastName",
            "in": "formData",
            "description": "Enter Last Name",
            "type": "string"
          },
          {
            "name": "Password",
            "in": "formData",
            "description": "Enter Password",
            "type": "string"
          },
          {
            "name": "DoB",
            "in": "formData",
            "description": "Enter Date of Birth",
            "required": false,
            "type": "string",
            "format": "date"
          },
          {
            "name": "PhoneNum",
            "in": "formData",
            "description": "Enter Phone Number",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Update Successfully"
          }
        }
      }
    },
    "/delete": {
      "delete": {
        "summary": "Delete Your Account",
        "produces": ["application/json"],
        "security": [
          {
            "JWTAuth": []
          }
        ],
        "parameters": [
          {
            "name": "EmailId",
            "in": "formData",
            "description": "Enter Email Id",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Delete Successfully"
          }
        }
      }
    }
  }
}
